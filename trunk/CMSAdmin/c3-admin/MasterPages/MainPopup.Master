			</ContentTemplate>
		</asp:UpdatePanel>
		<asp:ContentPlaceHolder ID="NoAjaxContentPlaceHolder" runat="server">
		</asp:ContentPlaceHolder>
	</div>
	<asp:PlaceHolder ID="plcFooter" runat="server" />
	<div style="display: none">
		<div id="cms-basic-modal-content">
